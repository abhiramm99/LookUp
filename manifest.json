{
    "manifest_version": 2,
    "name": "LookUp Quick Dictionary",
    "version": "0.1.0",
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.script.js"]
        }
    ],
    "background": {
        "scripts": [
            "background.script.js"
        ]
    },
    "browser_action": {
        "default_popup": "./popup/popup.html"
    },
    "permissions": [
        "contextMenus"
    ]
}